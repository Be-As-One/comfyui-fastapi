# 测试环境配置
APP_ENV=development
LOG_LEVEL=DEBUG
HOST=0.0.0.0
PORT=8000

# 任务API设置  
TASK_API_URL=http://localhost:8000/api

# ComfyUI设置（可选，如果没有ComfyUI实例可以注释掉）
# COMFYUI_URL=http://localhost:8188
# COMFYUI_CLIENT_ID=fastapi-test-client

# 存储提供商（测试时可以先用本地存储）
STORAGE_PROVIDER=local
# 如果要测试云存储，取消注释下面的配置：
# STORAGE_PROVIDER=gcs
# GCS_BUCKET_NAME=your-test-bucket
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/credentials.json

# FaceFusion设置
FACEFUSION_MODEL_PATH=/models
FACEFUSION_TEMP_DIR=/tmp/facefusion

# 调试设置
ENABLE_DEBUG=true
ENABLE_WEBSOCKET_LOGGING=true